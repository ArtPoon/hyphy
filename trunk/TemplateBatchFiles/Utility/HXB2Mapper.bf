_HXB2_Sequence_ = "";

alignOptions = {};
alignOptions ["SEQ_ALIGN_CHARACTER_MAP"]="ACGT";
alignOptions ["SEQ_ALIGN_SCORE_MATRIX"] = 	{
{5,-4,-4,-4}
{-4,5,-4,-4}
{-4,-4,5,-4}
{-4,-4,-4,5}
};
alignOptions ["SEQ_ALIGN_GAP_OPEN"]		= 	50;
alignOptions ["SEQ_ALIGN_GAP_OPEN2"]	= 	50;
alignOptions ["SEQ_ALIGN_GAP_EXTEND"]	= 	1;
alignOptions ["SEQ_ALIGN_GAP_EXTEND2"]	= 	1;
alignOptions ["SEQ_ALIGN_AFFINE"]		=   1;
alignOptions ["SEQ_ALIGN_NO_TP"]		=   1;

function mapSequenceToHXB2 (seq,option)
{
	_seqLen	  = Abs(seq);
	_coordMap = {_seqLen,1};
	
	_inStr 		 = {{_HXB2_Sequence_,seq}};
	AlignSequences(aligned, _inStr, alignOptions);
	
	_alignedHXB  = (aligned[0])[1];
	_alignedQRY  = (aligned[0])[2];
	
	_referenceStart = (_alignedHXB$"^\\-+")[0]+1;
	
	for (_k = 0; _k < _referenceStart; _k = _k+1)
	{
		_coordMap[_k] = _referenceStart-1;
	}
	
	_qryCoord = _k;
	_refCoord = _referenceStart;

	while (_k < Abs(_alignedQRY))
	{
		if (_alignedQRY[_k] != "-")
		{
			_coordMap[_qryCoord] = _refCoord;
			_qryCoord = _qryCoord + 1;
		}
		if (_alignedHXB[_k] != "-")
		{
			_refCoord = _refCoord + 1;
		}
		_k = _k+1;
	}
	return _coordMap;
}

